<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>hostel_master_tbl</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>Hostel Table</h1>
    <table>
        <thead>
            <tr>
                <th>Hostel_id</th>
                <th>Hostel Name</th>
                <th>No. of floors</th>
                <th>No. of rooms</th>
                
                <!-- Add more table headers if needed -->
            </tr>
        </thead>
        
        <tbody>
            <% data.forEach(item => { %>
                <tr>
                    <td><%= item.h_id %></td>
                    <td><%= item.h_name %></td>
                    <td><%= item.no_floors %></td>
                    <td><%= item.no_rooms %></td>
                    
                    <!-- Add more table data if needed -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
